<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/cell_giftcontributoritem_container"
    >
  <TextView
      android:id="@+id/rank_num"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="No.4"
      android:layout_alignParentLeft="true"
      android:layout_centerVertical="true"
      android:textColor="@color/text_color"
      android:textSize="15dp"
      android:layout_marginRight="10dp"
      android:visibility="gone"
      />
  <RelativeLayout
      android:layout_width="wrap_content"
      android:layout_height="fill_parent"
      android:layout_centerVertical="true"
      >

    <com.ilikezhibo.ggzb.xiangmu.RoundImageView
        xmlns:imagecontrol="http://schemas.android.com/apk/res-auto"
        android:id="@+id/user_portrait"
        android:layout_width="40.0dip"
        android:layout_height="40.0dip"
        android:layout_centerVertical="true"
        android:src="@drawable/default_head"
        imagecontrol:border_inside_color="@color/global_main_bg"
        imagecontrol:border_thickness="0.8dp"
        />
    <ImageView
        android:id="@+id/img_user_type"
        android:layout_width="13.0dip"
        android:layout_height="13.0dip"
        android:layout_alignBottom="@id/user_portrait"
        android:layout_alignRight="@id/user_portrait"
        android:src="@drawable/global_xing_1"
        android:visibility="gone"
        />
    <RelativeLayout
        android:id="@+id/infor_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/user_portrait"
        >
      <LinearLayout
          android:id="@+id/name_gender"
          android:layout_width="fill_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10.0dip"
          android:layout_marginTop="3dp"
          android:gravity="center_vertical"
          android:orientation="horizontal"
          >
        <TextView
            android:id="@+id/txt_username"
            android:text="@string/userhome_nickname_default"
            style="@style/cell_useritem_username"
            android:maxLength="12"/>
        <ImageView
            android:id="@+id/img_gender"
            android:contentDescription="@string/app_name"
            style="@style/cell_useritem_gender_img"
            android:visibility="gone"
            />

        <RelativeLayout
            android:id="@+id/grade_ly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="1dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="3dp"
            android:layout_marginTop="1.5dp"
            android:background="@drawable/icon_medal_32_1"
            android:gravity="center_vertical"
            android:visibility="gone"
            >
          <TextView
              android:id="@+id/grade_tv"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentTop="true"
              android:layout_centerVertical="true"
              android:layout_marginRight="3dp"
              android:gravity="center_vertical"
              android:singleLine="true"
              android:text="130"
              android:textColor="@color/white"
              android:textSize="12sp"
              />
        </RelativeLayout>

        <ImageView
            android:layout_width="14dp"
            android:layout_height="15dp"
            android:src="@drawable/record_time"
            android:layout_marginLeft="8dp"
            />
        <TextView
            android:id="@+id/record_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="3dp"
            android:layout_marginLeft="2dp"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text="2016-08-01"
            android:layout_weight="1"
            android:textColor="@color/graylight2"
            android:textSize="12sp"/>
        <ImageView
            android:layout_width="15dp"
            android:layout_height="13dp"
            android:src="@drawable/record_num"
            android:layout_marginLeft="8dp"
            />
        <TextView
            android:id="@+id/viewer_sum_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text="130"
            android:layout_marginLeft="2dp"
            android:textColor="@color/graylight2"
            android:textSize="12sp"/>
      </LinearLayout>
      <TextView
          android:id="@+id/txt_tip"
          android:layout_alignLeft="@id/name_gender"
          android:layout_below="@id/name_gender"
          android:layout_marginTop="2.0dip"
          android:text="#我的标题#"
          android:textSize="15sp"
          android:drawableLeft="@drawable/live_record_icon"
          android:drawablePadding="5dp"
          style="@style/cell_useritem_giftvalue"
          />
      <TextView
          android:id="@+id/txt_coin_count"
          android:layout_alignLeft="@id/name_gender"
          android:layout_below="@id/name_gender"
          android:layout_marginTop="0.0dip"
          android:text="@string/room_contributor_contribution"
          style="@style/gift_contributor_coin_count"
          android:visibility="gone"
          />

    </RelativeLayout>
  </RelativeLayout>

</RelativeLayout>