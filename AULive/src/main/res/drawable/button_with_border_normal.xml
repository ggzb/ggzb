<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <!-- 主体背景颜色值 -->
  <item
      android:bottom="1dp"
      android:left="1dp"
      android:right="1dp"
      android:top="1dp">
    <shape>
      <stroke
          android:color="@color/global_main_bg"
          android:width="1dp"/>
      <solid android:color="@color/global_main_bg" />
      <padding
          android:bottom="2dp"
          android:left="2dp"
          android:right="2dp"
          android:top="2dp"/>
      <corners android:radius="25dp"/>
    </shape>
  </item>
</layer-list>